{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.14.0/.schema/devbox.schema.json",
  "env": {
    "PATH": "${PATH}:${DEVBOX_PROJECT_ROOT}/bin"
  },
  "packages": {
    "rustup":            "1.28.2",
    "libiconv":          "latest",
    "static-web-server": "2.36.1",
    "libxml2": {
      "version": "2.13.8",
      "outputs": ["out", "dev"]
    }
  },
  "shell": {
    "init_hook": ["rustup default stable"]
  }
}
